{
	"name": "vueblog",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"fix": "vue-cli-service lint --fix"
	},
	"dependencies": {
		"apollo-boost": "^0.4.7",
		"core-js": "^3.4.4",
		"graphql": "^14.5.8",
		"graphql-tag": "^2.10.1",
		"moment": "^2.24.0",
		"register-service-worker": "^1.6.2",
		"vue": "^2.6.10",
		"vue-apollo": "^3.0.2",
		"vue-router": "^3.1.3",
		"vuetify": "^2.1.0",
		"vuex": "^3.1.2"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.1.0",
		"@vue/cli-plugin-eslint": "^4.1.0",
		"@vue/cli-plugin-pwa": "^4.1.0",
		"@vue/cli-service": "^4.1.0",
		"@vue/eslint-config-standard": "^4.0.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"eslint-plugin-import": "^2.20.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-vue": "^5.0.0",
		"node-sass": "^4.12.0",
		"prettier": "^1.19.1",
		"sass": "^1.19.0",
		"sass-loader": "^8.0.0",
		"vue-cli-plugin-vuetify": "^2.0.3",
		"vue-template-compiler": "^2.6.10",
		"vuetify-loader": "^1.3.0"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"es6": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/standard",
			"plugin:vue/base"
		],
		"rules": {
			"semi": "off",
			"no-plusplus": "off",
			"eol-last": "off",
			"max-len": [
				"error",
				150,
				{
					"ignoreUrls": true
				}
			],
			"no-tabs": "off",
			"prefer-destructuring": "off",
			"no-param-reassign": "off"
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
